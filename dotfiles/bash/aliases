# processes
alias tu='top -o %CPU' #cpu
alias tm='top -o %MEM' #memory

# fs
alias ..='cd ..'
alias cdd='cd -'

function take {
  mkdir $1
  cd $1
}

# git
alias g='git status'

# sdwan
alias sdwan_deploy="cd ~/WebstormProjects/sdwan_gui && npm run build && mv build sd-wan_gui_s2s && scp -r ./sd-wan_gui_s2s sdwan_dev:~/ && rm -rf ./sd-wan_gui_s2s"

# bash
alias reload_bash="source ~/.bashrc"
